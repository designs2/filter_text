{
  "name": "designs2/filter_text",
  "description": "MetaModels text filter",
  "keywords": [
    "contao",
    "metamodels",
    "filter",
    "text"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/filter_text/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/designs2/filter_text"
  },
  "require": {
    "php": ">=5.3",
    "contao/core": ">=3.2,<3.6",
    "contao-community-alliance/composer-plugin": "*",
    "designs2/core": "*"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "*",
    "phpcq/all-tasks": "*"
  },
  "conflict": {
    "contao/core": "3.4.0"
  },
  "autoload": {
    "psr-0": {
      "MetaModels": "src"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/metamodelsfilter_text"
      },
      "transifex": {
        "project": "metamodels",
        "prefix": "filter_text-",
        "languages_cto": "contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
